{
  "name": "demo",
  "description": "Bic website packaging framework demo.",
  "version": "0.0.0-1",
  "author": {
    "name": "Ray Patterson",
    "email": "github.raypatterson@gmail.com"
  },
  "dependencies": {
    "bootstrap-sass": "~3.3.7",
    "fastclick": "~1.0.6",
    "font-awesome": "~4.7.0",
    "hack-font": "~2.0.19"
  },
  "devDependencies": {
    "@bicjs/bic": "file:../../modules/bic",
    "@bicjs/bic-cli": "file:../../modules/bic-cli",
    "@bicjs/bic-config": "file:../../modules/bic-config",
    "@bicjs/bic-loader-fonts": "file:../../loaders/bic-loader-fonts",
    "@bicjs/bic-loader-images": "file:../../loaders/bic-loader-images",
    "@bicjs/bic-loader-modernizr": "file:../../loaders/bic-loader-modernizr",
    "@bicjs/bic-loader-scripts": "file:../../loaders/bic-loader-scripts",
    "@bicjs/bic-loader-styles": "file:../../loaders/bic-loader-styles",
    "@bicjs/bic-loader-templates": "file:../../loaders/bic-loader-templates",
    "@bicjs/bic-logger": "file:../../modules/bic-logger",
    "babel-preset-bic": "file:../../configs/babel-preset-bic",
    "eslint-config-bic": "file:../../configs/eslint-config-bic",
    "stylelint-config-bic": "file:../../configs/stylelint-config-bic"
  },
  "homepage": "https://github.com/bicjs/bic/blob/master/packages/test/demo/README.md",
  "keywords": [
    "app",
    "bic",
    "bicjs",
    "build",
    "demo",
    "framework",
    "webpack",
    "website"
  ],
  "main": ".",
  "scripts": {
    "build": "bic --clean --production --rc ./bic.production.config.js",
    "build:preview": "npm run build -- --open",
    "debug": "bic --debug",
    "dev": "bic",
    "link": "linklocal link -r && linklocal list -r",
    "test": "npm run test:pass",
    "test:fail": "exit 1",
    "test:pass": "exit 0",
    "unlink": "linklocal link -u -r && linklocal list"
  }
}
