  "version": "0.0.0-1",
	"devDependencies": {
    "commitizen": "~2.9.5",
    "cz-conventional-changelog": "~1.2.0",
    "eslint": "~3.14.0",
    "eslint-config-xo": "~0.17.0",
    "fixpack": "~2.3.1",
    "updtr": "~0.2.3"
  },
  "homepage": "https://github.com/bicjs/packages/passthrough-loader/",
  "keywords": [
    "loader",
