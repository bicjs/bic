{
  "name": "bic-demo",
  "description": "Bic website packaging framework demo.",
  "version": "0.0.0-1",
  "dependencies": {
    "bootstrap-sass": "~3.3.7",
    "fastclick": "~1.0.6",
    "font-awesome": "~4.7.0",
    "hack-font": "~2.0.19"
  },
  "devDependencies": {
    "@bicjs/bic": "git+ssh://git@github.com/bicjs/packages/bic#master",
    "commitizen": "~2.9.5",
    "cz-conventional-changelog": "~1.2.0",
    "eslint": "~3.14.0",
    "eslint-config-xo": "~0.17.0",
    "fixpack": "~2.3.1",
    "npm-autolink": "~0.4.1",
    "npm-link-check": "~1.2.0",
    "npm-run-all": "~4.0.1",
    "ntl": "~1.1.3",
    "updtr": "~0.2.3"
  },
  "homepage": "https://github.com/bicjs/bic/packages/bic-demo/",
  "keywords": [
    "bic",
    "bicjs",
    "webpack",
    "website",
    "build",
    "framework",
    "app",
		"demo"
  ],
  "main": "index.js",
  "scripts": {
    "build": "bic --clean --production --config ./bic.production.config.js",
    "commit": "git-cz",
    "dev": "bic",
    "link": "npm-autolink link",
    "link:rm": "npm-autolink remove",
    "lint": "eslint --fix src",
    "ls": "ntl .",
    "test": "npm-run-all --parallel test:*",
    "test:link": "npm-link-check",
    "upgrade": "updtr && fixpack"
  }
}
