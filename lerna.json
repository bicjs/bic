{
  "packages": [
    "packages/configs/*",
    "packages/tests/*",
    "packages/plugins/*",
    "packages/modules/*",
    "packages/utils/*"
  ],
  "command": {
    "publish": {
      "skipNpm": true,
      "message": "chore(release): publish %s"
    }
  },
  "npmClient": "yarn",
  "useWorkspaces": true,
  "exact:": true,
  "useGitVersion": true,
  "gitVersionPrefix": "v",
	"version": "independent"
}